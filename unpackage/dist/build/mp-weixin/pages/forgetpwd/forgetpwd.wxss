page,
.page-view {
  height: 100%;
  width: 100%;
  background: #81a3e2;
}
.step {
  padding-top: 300rpx;
}
.card {
  border-radius: 10rpx;
  padding: 35rpx 50rpx 60rpx;
  margin: 0 60rpx;
}
.card .title {
    text-align: center;
    color: #666;
    font-size: 34rpx;
}
.card .group {
    margin-top: 60rpx;
}
.card .group .row {
      line-height: 50rpx;
}
.card .group .row + .row {
        margin-top: 40rpx;
}
.card .group .row ._i {
        display: block;
        width: 40rpx;
        height: 40rpx;
        margin-right: 25rpx;
        background: no-repeat center center;
        background-size: contain;
}
.card .group .row ._i.icon1 {
          background-image: url("data:image/png;base64,");
}
.card .group .row ._i.icon2 {
          background-image: url("data:image/png;base64,");
}
.card .group .row ._i.icon3 {
          background-image: url("data:image/png;base64,");
}
.card .group .row input {
        border-bottom: 1px solid #f7f7fc;
        font-size: 30rpx;
}
.card .group .row .getcode {
        color: #fff;
        background: #4b6aef;
        border-radius: 6rpx;
        width: 140rpx;
        line-height: 50rpx;
        text-align: center;
        font-size: 20rpx;
}
.stepBtn {
  font-size: 40rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  width: 540rpx;
  margin: 60rpx auto 0;
  background: #4b6aef;
  border-radius: 100rpx;
}

